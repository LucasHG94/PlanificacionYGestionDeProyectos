<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="GestionProyectosPU" transaction-type="JTA">
    <jta-data-source>pgpR</jta-data-source>
    <class>modelo.Actividad</class>
    <class>modelo.Administrador</class>
    <class>modelo.Categoriaroles</class>
    <class>modelo.Datosconfigurables</class>
    <class>modelo.Dedicacion</class>
    <class>modelo.Etapa</class>
    <class>modelo.Informesemanal</class>
    <class>modelo.Proyecto</class>
    <class>modelo.Trabajador</class>
    <class>modelo.Vacaciones</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
